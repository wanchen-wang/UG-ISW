class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        sorted_s = sorted(s)
        long_streak = 0
        for ss in sorted_s:
            hashmap = list()
            if ss not in hashmap:
                hashmap.append(ss)
                current_long = 1

                while chr(ord(ss) + 1) not in hashmap:
                    current_long += 1
                    hashmap.append(chr(ord(ss) + 1))
                long_streak = max(long_streak, current_long)
            

        return long_streak